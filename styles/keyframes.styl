@keyframes headingText
  0%
    opacity 0
    left -100%

  100%
    opacity 1
    left 0

@keyframes headingLine
  0%
    opacity 0
    left 100%

  100%
    opacity 1
    left 0

@keyframes fadeInBottom
  0%
    opacity 0
    transform translateY(40px)

  100%
    opacity 1
    transform translateY(0)